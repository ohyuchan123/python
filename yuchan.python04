#함수
#리턴
#자료없이 리턴하기
def return_test():
    print("A 위치입니다")
    return
    print("B 위치입니다")

#함수 호출
return_test()

#자료와 함꼐 리턴하기
def return_test():
    print()
    return 100
#함수 호출
value=return_test()
print(value)

#아무것도 리턴하지 않기
def return_test():
    print()
    return
#함수 호출
value=return_test()
print(value)

